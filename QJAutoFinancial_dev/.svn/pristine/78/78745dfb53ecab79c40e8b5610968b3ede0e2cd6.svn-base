package com.qianjing.finance.ui.activity.card;

import java.util.ArrayList;

import com.qianjing.finance.model.common.Bank;
import com.qianjing.finance.model.common.BrachBank;


public class ContentValueBound {
	public static String boundbranch;
	public static String bankinfo;// 存放读取的bank.json的字符串，省市级县级信息
	public static ArrayList<Bank> bankList;// 绑定选择银行列表
	public static String levelOneKey;// 一级选择关键
	public static String levelTwoKey;// 二级选择关键
	public static String levelThreeKey;// 三级选择关键
	public static String selectedBank;// 选择的银
	public static boolean isSelectBank = false;// true为选中
	public static int selectBankItemNum;// 选中的条
	public static Bank selBank;// 选择的银行相关对
	public static String inputCardNum;// 输入的卡号信
	public static BrachBank selBrachBank;// 记录选中的支

	public static String levelOneString;// 一级列
	public static String levelTwoString;// 二级列表
	public static String levelThreeString;// 三级列表

	public static String dialID;// 商户订单号（绑卡提交返回返回订单号）

	public static boolean isFromCardManager = false;// 绑卡入口标记
	public static String payPlugBackData;// 提交订单返回数据

	// 清空静态数
	public static void clearData() {
		ContentValueBound.boundbranch = null;
		ContentValueBound.bankinfo = null;
		ContentValueBound.bankList = null;
		ContentValueBound.levelOneKey = null;
		ContentValueBound.levelTwoKey = null;
		ContentValueBound.levelThreeKey = null;
		ContentValueBound.selectedBank = null;
		ContentValueBound.isSelectBank = false;
		// ContentValueBound.selectBankItemNum=null;
		ContentValueBound.selBank = null;
		ContentValueBound.inputCardNum = null;
		ContentValueBound.selBrachBank = null;

		ContentValueBound.levelOneString = null;
		ContentValueBound.levelTwoString = null;
		ContentValueBound.levelThreeString = null;
		ContentValueBound.dialID = null;
		ContentValueBound.isFromCardManager = false;
		ContentValueBound.payPlugBackData = null;
	}
}
