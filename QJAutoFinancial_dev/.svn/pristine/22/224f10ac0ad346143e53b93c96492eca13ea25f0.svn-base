<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/title_bar"
    style="@style/title" >

    <TextView
        android:id="@+id/title_middle_text"
        style="@style/titleText" />

    <Button
        android:id="@+id/bt_back"
        style="@style/imageNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="7dip"
        android:background="@drawable/title_left_but"
        android:paddingLeft="7dip"
        android:paddingRight="5dip"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/title_right_image"
        style="@style/imageNormal"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="5.0dip"
        android:paddingLeft="10dip"
        android:paddingRight="10dip"
        android:visibility="gone" />

    <Button
        android:id="@+id/ButReg"
        style="@style/textNormal"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5dip"
        android:background="#00000000"
        android:gravity="center"
        android:paddingLeft="10dip"
        android:paddingRight="5dip"
        android:text="注册"
        android:textColor="@drawable/white"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/ButModify"
        style="@style/TimageNormal"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dip"
        android:background="@drawable/title_edit"
        android:paddingLeft="10dip"
        android:paddingRight="10dip"
        android:visibility="gone" />

    <TextView
        android:id="@+id/right_title_view"
        style="@style/TimageNormal"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dip"
        android:paddingLeft="10dip"
        android:paddingRight="10dip"
        android:text="@string/str_finish"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/im_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dip"
        android:src="@drawable/cpay_title_btn2"
        android:visibility="gone" />
	
    <LinearLayout
        android:id="@+id/ll_assets_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:orientation="horizontal"
        android:visibility="gone" >

        <TextView
            android:id="@+id/tv_real_trade"
            style="@style/assetsTitleBar"
            android:layout_height="30dp"
            android:layout_width="120dp"
            android:background="@drawable/selector_assets_title"
            android:text="@string/real_assets" />
        
		
        <TextView
            android:id="@+id/tv_virtual_trade"
            style="@style/assetsTitleBar"
            android:layout_marginLeft="20dp"
            android:layout_height="30dp"
            android:layout_width="120dp"
            android:background="@drawable/selector_assets_title"
            android:text="@string/virtual_assets" />
    </LinearLayout>

</RelativeLayout>