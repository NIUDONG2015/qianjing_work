<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_e9e8ee" >

    <com.qianjing.finance.widget.pulltorefresh.PullToRefreshListView
        android:id="@+id/fund_search_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="@color/color_00000000"
        android:divider="@color/color_e9e8ee"
        android:dividerHeight="5dp"
        android:fadeScrollbars="false"
        android:scrollbars="none"
        android:scrollingCache="false" />

</RelativeLayout>