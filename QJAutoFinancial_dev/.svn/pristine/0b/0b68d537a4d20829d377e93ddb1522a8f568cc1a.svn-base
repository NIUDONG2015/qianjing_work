<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/layoutMain">
    
    <include 
        layout="@layout/include_title"/>
    
    <LinearLayout 
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:orientation="vertical">
    
    <LinearLayout 
        android:layout_height="0dp"
        android:layout_weight="10"
        android:layout_width="match_parent"
        android:orientation="vertical">
        
        <LinearLayout
            android:id="@+id/indicator_item_title"
            android:layout_height="50dp"
            android:layout_width="wrap_content"
            android:orientation="horizontal">
            
			<ImageView
			    android:id="@+id/iv_title_one"
			    android:layout_height="wrap_content"
			    android:layout_width="0dp"
			    android:layout_weight="1"
			    android:scaleType="fitEnd"
			    android:src="@drawable/custom_btn_selector_one"/>
			<ImageView
			    android:id="@+id/iv_title_two"
			    android:layout_height="wrap_content"
			    android:layout_width="0dp"
			    android:layout_weight="1"
			    android:scaleType="fitEnd"
			    android:layout_marginLeft="1dp"
			    android:src="@drawable/custom_btn_selector_two"/>
			<ImageView
			    android:id="@+id/iv_title_three"
			    android:layout_height="wrap_content"
			    android:layout_width="0dp"
			    android:layout_weight="1"
			    android:scaleType="fitEnd"
			    android:layout_marginLeft="1dp"
			    android:src="@drawable/custom_btn_selector_three"/>
			<ImageView
			    android:id="@+id/iv_title_four"
			    android:layout_height="wrap_content"
			    android:layout_width="0dp"
			    android:layout_weight="1"
			    android:scaleType="fitEnd"
			    android:layout_marginLeft="1dp"
			    android:src="@drawable/custom_btn_selector_four"/>
			<ImageView
			    android:id="@+id/iv_title_five"
			    android:layout_height="wrap_content"
			    android:layout_width="0dp"
			    android:layout_weight="1"
			    android:scaleType="fitEnd"
			    android:layout_marginLeft="1dp"
			    android:src="@drawable/custom_btn_selector_five"/>
			
        </LinearLayout>
        
        <com.qianjing.finance.view.custom.CtrlViewPager
            android:id="@+id/cvp_view_pager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
     		android:overScrollMode="never"/>
        
    </LinearLayout>
    
    <Button
        android:id="@+id/btn_continue"
        style="@style/VIPBtnNormal"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:text="继续"/>
    </LinearLayout>
</LinearLayout>
