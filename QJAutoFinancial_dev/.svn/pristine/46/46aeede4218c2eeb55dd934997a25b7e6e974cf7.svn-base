<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <clip>
            <shape>
                <corners android:radius="10dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@drawable/gray_background"
                    android:centerY="0.75"
                    android:endColor="@drawable/gray_background"
                    android:startColor="@drawable/gray_background" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dip" />

                <stroke
                    android:dashGap="0.0dip"
                    android:dashWidth="3.0dip"
                    android:width="1.0dip"
                    android:color="@drawable/pink_front" />

                <solid android:color="@drawable/pink_front" />
            </shape>
        </clip>
    </item>

</layer-list>